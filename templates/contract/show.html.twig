{% extends 'base.html.twig' %}

{% block title %}Contrat{% endblock %}

{% block body %}
    <h1>Contrat</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contrat.id }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ contrat.createdAt ? contrat.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FinishedAt</th>
                <td>{{ contrat.finishedAt ? contrat.finishedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Mission</th>
                <td>{{ contrat.mission }}</td>
            </tr>
            <tr>
                <th>Noteperso</th>
                <td>{{ contrat.noteperso }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_contract_index') }}">back to list</a>

    <a href="{{ path('app_contract_edit', {'id': contrat.id}) }}">edit</a>

    {{ include('contract/_delete_form.html.twig') }}
{% endblock %}
